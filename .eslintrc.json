{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "warn",
    "react/no-unescaped-entities": "off",
    "@next/next/no-img-element": "off",
    "prettier/prettier": ["warn", {
      "endOfLine": "auto",
      "singleQuote": true
    }]
  }
}
